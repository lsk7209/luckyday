{
  "name": "luckyday",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "cf:dev": "wrangler dev src/workers/index.ts",
    "cf:deploy": "wrangler deploy src/workers/index.ts",
    "cf:deploy:staging": "wrangler deploy --config wrangler.staging.toml",
    "cf:deploy:prod": "wrangler deploy --config wrangler.production.toml",
    "cf:db:migrate": "wrangler d1 execute cms-calculator-db --file=src/workers/schema.sql",
    "cf:db:migrate:staging": "wrangler d1 execute cms-calculator-staging --config wrangler.staging.toml --file=src/workers/schema.sql",
    "cf:db:migrate:prod": "wrangler d1 execute cms-calculator-prod --config wrangler.production.toml --file=src/workers/schema.sql",
    "cf:db:seed": "wrangler d1 execute cms-calculator-db --file=src/workers/seed.sql",
    "cf:db:seed:staging": "wrangler d1 execute cms-calculator-staging --config wrangler.staging.toml --file=src/workers/seed.sql",
    "cf:db:seed:prod": "wrangler d1 execute cms-calculator-prod --config wrangler.production.toml --file=src/workers/seed.sql",
    "cf:dream:schema": "wrangler d1 execute luckyday-db --file=cloudflare-workers/dream-schema.sql",
    "cf:dream:seed": "wrangler d1 execute luckyday-db --file=cloudflare-workers/dream-seed.sql",
    "cf:dream:seed:full": "wrangler d1 execute luckyday-db --file=cloudflare-workers/dream-seed-full.sql",
    "cf:dream:seed:extended": "wrangler d1 execute luckyday-db --file=cloudflare-workers/dream-seed-extended-complete.sql",
    "cf:dream:seed:viral": "wrangler d1 execute luckyday-db --file=cloudflare-workers/dream-seed-viral-enriched.sql",
    "cf:dream:seed:ultra": "wrangler d1 execute luckyday-db --file=cloudflare-workers/dream-seed-viral-ultra.sql",
    "cf:dream:seed:ultimate": "wrangler d1 execute luckyday-db --file=cloudflare-workers/dream-seed-ultimate-viral.sql",
    "cf:dream:seed:mega": "wrangler d1 execute luckyday-db --file=cloudflare-workers/dream-seed-mega-viral.sql",
    "cf:dream:seed:super": "wrangler d1 execute luckyday-db --file=cloudflare-workers/dream-seed-super-viral.sql",
    "cf:dream:seed:hyper": "wrangler d1 execute luckyday-db --file=cloudflare-workers/dream-seed-hyper-viral.sql",
    "cf:dream:seed:all": "npm run cf:dream:schema && npm run cf:dream:seed && npm run cf:dream:seed:full && npm run cf:dream:seed:extended && npm run cf:dream:seed:viral && npm run cf:dream:seed:ultra && npm run cf:dream:seed:ultimate && npm run cf:dream:seed:mega && npm run cf:dream:seed:super && npm run cf:dream:seed:hyper",
    "cf:dream:schema:local": "wrangler d1 execute luckyday-db --local --file=cloudflare-workers/dream-schema.sql",
    "cf:dream:seed:local": "wrangler d1 execute luckyday-db --local --file=cloudflare-workers/dream-seed.sql",
    "cf:dream:seed:full:local": "wrangler d1 execute luckyday-db --local --file=cloudflare-workers/dream-seed-full.sql",
    "cf:dream:seed:extended:local": "wrangler d1 execute luckyday-db --local --file=cloudflare-workers/dream-seed-extended-complete.sql",
    "cf:dream:seed:ultimate:local": "wrangler d1 execute luckyday-db --local --file=cloudflare-workers/dream-seed-ultimate-viral.sql",
    "cf:dream:seed:mega:local": "wrangler d1 execute luckyday-db --local --file=cloudflare-workers/dream-seed-mega-viral.sql",
    "cf:dream:seed:super:local": "wrangler d1 execute luckyday-db --local --file=cloudflare-workers/dream-seed-super-viral.sql",
    "cf:dream:seed:hyper:local": "wrangler d1 execute luckyday-db --local --file=cloudflare-workers/dream-seed-hyper-viral.sql",
    "cf:dream:seed:all:local": "npm run cf:dream:schema:local && npm run cf:dream:seed:local && npm run cf:dream:seed:full:local && npm run cf:dream:seed:extended:local && npm run cf:dream:seed:viral:local && npm run cf:dream:seed:ultra:local && npm run cf:dream:seed:ultimate:local && npm run cf:dream:seed:mega:local && npm run cf:dream:seed:super:local && npm run cf:dream:seed:hyper:local",
    "cf:dream:seed:viral:local": "wrangler d1 execute luckyday-db --local --file=cloudflare-workers/dream-seed-viral-enriched.sql",
    "cf:dream:seed:ultra:local": "wrangler d1 execute luckyday-db --local --file=cloudflare-workers/dream-seed-viral-ultra.sql",
    "cf:tail": "wrangler tail",
    "cf:tail:staging": "wrangler tail --config wrangler.staging.toml",
    "cf:tail:prod": "wrangler tail --config wrangler.production.toml",
    "pages:build": "next build",
    "pages:deploy": "npm run pages:build && npx wrangler@latest pages deploy out",
    "pages:deploy:staging": "npm run pages:build && npx wrangler@latest pages deploy out --branch staging",
    "pages:deploy:prod": "npm run pages:build && npx wrangler@latest pages deploy out --branch main",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "lhci autorun --config=.lighthouserc.json",
    "supabase:gen-types": "npx supabase gen types typescript --project-id YOUR_PROJECT_ID > src/types/supabase.ts",
    "db:seed": "npx supabase db reset --db-url YOUR_DB_URL",
    "db:migrate": "npx supabase db push --db-url YOUR_DB_URL"
  },
  "dependencies": {
    "@fontsource/pretendard": "^5.2.5",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.2",
    "@tanstack/react-query": "^5.90.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "openai": "^4.67.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "wrangler": "^4.45.3"
  }
}
